{
  "version": 2,
  "builds": [
    {
      "src": "api/order.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/send.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/order",
      "dest": "api/order.js"
    },
    {
      "src": "/api/send",
      "dest": "api/send.js"
    }
  ]
}
